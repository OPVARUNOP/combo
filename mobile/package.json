{
  "name": "combo-mobile",
  "version": "1.0.0",
  "private": true,
  "main": "index.js",
  "scripts": {
    "android": "expo run:android",
    "ios": "expo run:ios",
    "start": "expo start",
    "build": "expo export --platform web",
    "build:web": "expo build:web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "eject": "expo eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "jest --config=jest.config.js",
    "test:watch": "jest --watch --config=jest.config.js",
    "test:coverage": "jest --coverage --config=jest.config.js",
    "type-check": "tsc --noEmit",
    "clean": "expo start --clear",
    "postinstall": "echo 'Installation completed'",
    "prebuild": "echo 'Preparing build...'",
    "build:dev": "expo build:android --type apk",
    "build:prod": "expo build:android --type app-bundle",
    "deploy:staging": "expo publish --release-channel staging",
    "deploy:production": "expo publish --release-channel production",
    "analyze": "npx expo-bundle-analyzer"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@expo/webpack-config": "^19.0.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/netinfo": "9.3.10",
    "@react-native-community/slider": "^5.0.1",
    "@react-native-firebase/app": "^23.4.0",
    "@react-native-firebase/auth": "^23.4.0",
    "@react-native-firebase/firestore": "^23.4.0",
    "@react-navigation/bottom-tabs": "6.5.11",
    "@react-navigation/native": "6.1.9",
    "@react-navigation/native-stack": "6.9.17",
    "@react-navigation/stack": "6.3.20",
    "@reduxjs/toolkit": "2.0.1",
    "axios": "1.6.2",
    "expo": "49.0.21",
    "expo-analytics-amplitude": "~11.3.0",
    "expo-app-loading": "~2.1.0",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.13",
    "expo-device": "~5.4.0",
    "expo-linking": "~5.0.2",
    "expo-localization": "~14.3.0",
    "expo-notifications": "~0.20.1",
    "expo-status-bar": "^3.0.8",
    "expo-store-review": "~6.4.0",
    "firebase": "^12.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.10",
    "react-native-bootsplash": "5.1.3",
    "react-native-dotenv": "^3.4.11",
    "react-native-fast-image": "8.6.3",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-orientation-locker": "1.6.0",
    "react-native-paper": "^5.14.5",
    "react-native-permissions": "3.10.1",
    "react-native-reanimated": "~3.3.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-share": "9.4.1",
    "react-native-super-grid": "5.0.0",
    "react-native-swiper-flatlist": "3.2.3",
    "react-native-track-player": "^4.0.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-voice": "0.3.0",
    "react-native-web": "0.19.9",
    "react-native-worklets": "0.5.1",
    "react-redux": "9.0.4",
    "redux-persist": "6.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.6",
    "@babel/preset-env": "7.23.6",
    "@babel/runtime": "7.23.6",
    "@react-native-community/cli": "12.3.0",
    "@react-native/eslint-config": "0.72.2",
    "@react-native/metro-config": "0.72.11",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@tsconfig/react-native": "3.0.2",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.25",
    "@types/react-native": "^0.72.8",
    "@types/react-test-renderer": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "29.7.0",
    "eslint": "8.56.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "29.7.0",
    "jest-expo": "^54.0.12",
    "metro-react-native-babel-preset": "0.77.0",
    "prettier": "^2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "expo": {
    "doctor": {
      "enabled": true
    }
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.(ts|tsx|js)",
      "**/?(*.)+(spec|test).(ts|tsx|js)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  }
}
